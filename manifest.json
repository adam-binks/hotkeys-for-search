{
  "manifest_version": 3,
  "name": "Search Shortcuts",
  "version": "1.0",
  "description": "Quickly search using hotkeys",
  "permissions": ["activeTab", "commands", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "search1": {
      "suggested_key": {
        "default": "Alt+Q"
      },
      "description": "Google Search"
    },
    "search2": {
      "suggested_key": {
        "default": "Alt+W"
      },
      "description": "Wikipedia Search"
    },
    "search3": {
      "suggested_key": {
        "default": "Alt+S"
      },
      "description": "Google Scholar Search"
    }
  },
  "default_locale": "en"
}
